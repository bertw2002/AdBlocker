{
    "author": "Albert Wan",
    "manifest_version": 2,
    "name": "Twitch AdBlocker",
    "description": "block ads for video game players",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Twitch AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.twitch.tv/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}
